<phpunit
  xlmns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/6.3/phpunit.xsd"
  backupGlobals="true"
  backupStaticAttributes="false"
  bootstrap="vendor/autoload.php"
  cacheTokens="false"
  colors="true"
  convertErrorsToExeptions="true"
  convertnoticesToExeptions="true"
  convertWarningsToExeptions="true"
  forceCoversAnnotations="false"
  mapTestClassNameToConverdClassName="false"
  processIsolation="false"
  stopOnError="false"
  stopOnFailure="false"
  stopOnIncomplete="false"
  stopOnSkipped="false"
  stopOnRisky="false"
  timeoutForSmallTest="1"
  timeoutForMediumTest="10"
  timeoutForLargeTest="60"
  verbose="false"
</phpunit>